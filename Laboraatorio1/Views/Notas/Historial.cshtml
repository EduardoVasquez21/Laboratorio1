@model List<Laboraatorio1.Models.TblNotasEstudiante>
@{
    ViewBag.Title = "Historial";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Historial</h2>


<table class="table table-striped table-dark">
    <thead>
    <th>
        Id
    </th>

    <th>
        Nombre
    </th>

    <th>
        Lab 1
    </th>

    <th>
        Lab 2
    </th>
    <th>
        Lab 3
    </th>

    <th>
        Parcial 1
    </th>

    <th>
        Parcial 2
    </th>

    <th>
        Parcial3
    </th>

    </thead>

    <tbody>

        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Id</td>
                <td>@item.Nombre</td>
                <td>@item.lab1</td>
                <td>@item.lab2</td>
                <td>@item.lab3</td>
                <td>@item.par1</td>
                <td>@item.par2</td>
                <td>@item.par3</td>

            </tr>


        }
    </tbody>

</table>
